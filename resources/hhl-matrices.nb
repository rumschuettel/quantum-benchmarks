       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"\"\<B\>\"", "\[Rule]", 
         RowBox[{"U", "[", 
          RowBox[{"[", 
           RowBox[{
            RowBox[{
             RowBox[{"-", "blockSize"}], ";;"}], ",", 
            RowBox[{
             RowBox[{"-", "blockSize"}], ";;"}]}], "]"}], "]"}]}], "//", 
        "Chop"}], ",", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{
         RowBox[{"\"\<Histogram\>\"", "\[Rule]", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Table", "[", 
             RowBox[{
              RowBox[{
